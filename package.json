{
  "name": "svelte-preprocess-sass",
  "version": "1.0.0",
  "description": "Svelte preprocessor for sass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ls-age/svelte-preprocess-sass.git"
  },
  "author": "Lukas Hechenberger <npm@ls-age.com>",
  "homepage": "https://github.com/ls-age/svelte-preprocess-sass#readme",
  "bugs": {
    "url": "https://github.com/ls-age/svelte-preprocess-sass/issues"
  },
  "main": "out/index.js",
  "module": "out/module.js",
  "scripts": {
    "build": "npm run -s compile",
    "compile": "rollup -c",
    "dev": "rollup -cw",
    "format": "prettier --ignore-path .gitignore .",
    "lint": "eslint rollup.config.js src test",
    "test": "nyc ava"
  },
  "keywords": [
    "svelte",
    "preprocess",
    "sass"
  ],
  "dependencies": {
    "svelte-preprocess-filter": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.3",
    "@babel/preset-env": "7.14.2",
    "@babel/register": "7.13.16",
    "@ls-age/bump-version": "0.2.1",
    "@ls-age/eslint-config": "0.9.2",
    "ava": "3.15.0",
    "codecov": "3.8.2",
    "eslint": "7.19.0",
    "eslint-config-prettier": "8.3.0",
    "node-sass": "6.0.0",
    "nyc": "15.1.0",
    "prettier": "2.3.0",
    "prettier-plugin-package": "1.3.0",
    "rollup": "2.50.0",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-node-resolve": "5.2.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": 10
          }
        }
      ]
    ],
    "env": {
      "test": {
        "sourceMaps": "inline"
      }
    }
  },
  "directories": {
    "test": "test"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "@ls-age",
      "prettier"
    ],
    "settings": {
      "import/core-modules": [
        "node-sass"
      ]
    },
    "rules": {
      "compat/compat": "off"
    }
  },
  "renovate": {
    "extends": [
      "@ls-age:automergeDev"
    ]
  }
}
