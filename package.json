{
  "name": "svelte-preprocess-sass",
  "version": "0.2.0-beta.0",
  "description": "Svelte preprocessor for sass",
  "main": "out/index.js",
  "module": "out/module.js",
  "dependencies": {
    "svelte-preprocess-filter": "^1.0.0"
  },
  "devDependencies": {
    "@ls-age/babel-preset": "^0.4.0",
    "@ls-age/bump-version": "^0.1.2",
    "@ls-age/eslint-config": "^0.8.0",
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "codecov": "^3.0.2",
    "eslint": "^4.19.1",
    "node-sass": "^4.9.0",
    "nyc": "^12.0.2",
    "rollup": "^0.60.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "scripts": {
    "compile": "rollup -c",
    "lint": "eslint rollup.config.js src test",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ls-age/svelte-preprocess-sass.git"
  },
  "keywords": [
    "svelte",
    "preprocess",
    "sass"
  ],
  "author": "Lukas Hechenberger <npm@ls-age.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ls-age/svelte-preprocess-sass/issues"
  },
  "homepage": "https://github.com/ls-age/svelte-preprocess-sass#readme",
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "extends": "@ls-age",
    "settings": {
      "import/core-modules": [
        "node-sass"
      ]
    },
    "rules": {
      "compat/compat": "off"
    }
  },
  "babel": {
    "presets": [
      [
        "@ls-age/babel-preset",
        {
          "modules": false
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "@ls-age/babel-preset"
        ],
        "sourceMaps": "inline"
      }
    }
  },
  "ava": {
    "require": [
      "babel-core/register"
    ]
  },
  "directories": {
    "test": "test"
  }
}
